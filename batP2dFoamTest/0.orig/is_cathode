FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      is_cathode;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];//[kg m s K mol A cd]

internalField   uniform 0;

boundaryField
{
    ".*"
    {
        type            calculated;
        value           $internalField;
    }
    
    
    frontAndBack
    {
        type            empty;
    }
    
    topAndBottom
    {
        type            empty;
    }    
    
    
}


